

  <h1>Blog Posts</h1>

  <a href = "/add"> Create New Post </a>
  </br></br>

  {{#each post}}
    <div class="post">
        <div class="post-title">{{title}}</div>
        <div class="post-date">{{date}}</div>
        <div class="post-content">
        <p>
            {{content}}
        </p>
        </div>
    </div>
    {{else}}
        No posts
    {{/each}}

