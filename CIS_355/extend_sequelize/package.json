{
  "name": "book-library",
  "version": "1.0.0",
  "description": "A book library management system using Express, Sequelize, and Handlebars",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "clean": "if exist dist (rmdir /s /q dist) else (echo unable to find directory)",
    "prebuild": "npm run clean"    
  },
  "keywords": [
    "express",
    "sequelize",
    "typescript",
    "handlebars",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.3",
    "sequelize": "^6.29.3",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-handlebars": "^6.0.2",
    "@types/node": "^18.15.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
